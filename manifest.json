{
    "manifest_version": 2,
    "name": "SmartMarks",
    "version": "1.1",
    "description": "Suggesting a folder to save a bookmark",
    "homepage_url": "https://github.com/gopiprasanthpotipireddy/smartmarks",
    "icons": {
      "48": "icons/bookmark-it.png",
      "96": "icons/bookmark-it@2x.png"
    },
  
    "permissions": [
      "bookmarks",
      "tabs"
    ],
  
    "browser_action": {
      "default_icon": "icons/star-empty-38.png",
      "default_title": " SmartMarks"
    },
  
    "background": {
      "scripts": ["background.js"]
    }
  
  }